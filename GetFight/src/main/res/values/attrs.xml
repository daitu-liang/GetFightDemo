<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AspectRatioLayout">
        <attr name="oakAspectRatio" format="string" />
    </declare-styleable>
    <declare-styleable name="AnimatedSvgView">
        <attr name="oakSvgImageSizeX" format="integer" />
        <attr name="oakSvgImageSizeY" format="integer" />
        <attr name="oakSvgTraceTime" format="string" />
        <attr name="oakSvgTraceTimePerGlyph" format="string" />
        <attr name="oakSvgFillStart" format="string" />
        <attr name="oakSvgFillTime" format="string" />
    </declare-styleable>

    <!--底部导航-->
    <declare-styleable name="BottomNavigationBar">
        <attr name="bnbElevation" format="dimension" />
        <attr name="bnbActiveColor" format="color" />
        <attr name="bnbInactiveColor" format="color" />
        <attr name="bnbBackgroundColor" format="color" />
        <attr name="bnbAnimationDuration" format="integer" />
        <attr name="bnbMode" format="enum">
            <enum name="mode_default" value="0" />
            <enum name="mode_fixed" value="1" />
            <enum name="mode_shifting" value="2" />
        </attr>
        <attr name="bnbBackgroundStyle" format="enum">
            <enum name="background_style_default" value="0" />
            <enum name="background_style_static" value="1" />
            <enum name="background_style_ripple" value="2" />
        </attr>
        <attr name="bnbAutoHideEnabled" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="ImageCoverFlowView">
    <attr name="visibleImage" format="integer" />
    <attr name="reflectionHeight" format="fraction" />
    <attr name="reflectionGap" format="dimension" />
    <attr name="topImageClickEnable" format="boolean" />
    <attr name="coverflowGravity" format="enum">
        <enum name="top" value="0" />
        <enum name="bottom" value="1" />
        <enum name="center_vertical" value="2" />
    </attr>
    <attr name="coverflowLayoutMode" format="enum">
        <enum name="match_parent" value="0"></enum>
        <enum name="wrap_content" value="1"></enum>
    </attr>
    </declare-styleable>
</resources>